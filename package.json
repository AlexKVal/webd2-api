{
  "name": "webd2-api",
  "version": "0.0.1",
  "description": "json api server for the webd2",
  "bin": "bin/webd2-server",
  "files": [
    "LICENSE",
    "README.md",
    "lib",
    "bin"
  ],
  "scripts": {
    "lint": "standard",
    "tests-cmd": "babel-tape-runner tests/**/*-test.js | faucet",
    "test": "npm run lint && npm run tests-cmd",
    "tdd": "nodemon -q -x 'npm run tests-cmd'",
    "build": "rimraf lib && babel src -d lib",
    "start": "DEBUG=webd2-api:* nodemon --watch src -x 'npm run build && node bin/webd2-server'",
    "watch": "nodemon --watch src -x 'npm run build'",
    "win": "set DEBUG=webd2-api:* && node bin/webd2-server"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:alexkval/webd2-api.git"
  },
  "author": {
    "name": "Alexander Shemetovsky",
    "email": "alexkval@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alexkval/webd2-api/issues"
  },
  "homepage": "https://github.com/alexkval/webd2-api#readme",
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-eslint": "^5.0.0",
    "babel-preset-es2015": "6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-tape-runner": "^2.0.1",
    "blue-tape": "^0.2.0",
    "faucet": "0.0.1",
    "nodemon": "^1.9.0",
    "rimraf": "^2.5.2",
    "standard": "^6.0.7"
  },
  "dependencies": {
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "jsonapi-serializer": "^2.0.4",
    "morgan": "~1.6.1"
  }
}
